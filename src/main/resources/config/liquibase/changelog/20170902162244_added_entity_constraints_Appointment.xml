<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Appointment.
    -->
    <changeSet id="20170902162244-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="patient_id"
                                 baseTableName="appointment"
                                 constraintName="fk_appointment_patient_id"
                                 referencedColumnNames="id"
                                 referencedTableName="patient"/>

        <addForeignKeyConstraint baseColumnNames="doctor_schedule_id"
                                 baseTableName="appointment"
                                 constraintName="fk_appointment_doctor_schedule_id"
                                 referencedColumnNames="id"
                                 referencedTableName="doctor_schedule"/>

    </changeSet>
</databaseChangeLog>
